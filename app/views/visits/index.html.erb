<p id="notice"><%= notice %></p>

<h1>Visits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visits.each do |visit| %>
      <tr>
        <td><%= link_to 'Show', location_visit_path(@location, visit) %></td>
        <td><%= link_to 'Edit', edit_location_visit_path(@location, visit) %></td>
        <td><%= link_to 'Destroy', location_visit_path(@location, visit), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit', new_location_visit_path %>
