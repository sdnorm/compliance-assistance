<p id="notice"><%= notice %></p>

<p>
  <strong>Location:</strong>
  <%= @visit.location_id %>
</p>

<ul>
  <strong>Questions:</strong>
  <% @visit.questions.each do |q| %>
    <li>
      <%= q.content %> - 
      <% q.answers.each do |a| %>
        <%= a.name %> :: <%= a.content %>
      <% end %>
    </li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_location_visit_path(@visit) %> |
<%= link_to 'Back', location_visits_path %>
